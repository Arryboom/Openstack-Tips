define("question:widget/ask/search-relate/search-relate",function(e,a,t){function i(){n.addKey({searchRelateNew:1,isQ:!!window.isQualityView}),s(".wgt-search-relate .word").on("click",function(){if(s(this).hasClass("word-search")){n.send({page:"question",area:"search-relate-word-search",action:"click",type:2014,isQ:!!window.isQualityView});var e=s(".wgt-search-relate .word.selected"),a=window.F.context("page").title;if(e.length){var t=[];s.each(e,function(e,a){t.push(s(a).text())}),a=t.join(" ")}window.open("http://www.baidu.com/s?wd="+encodeURIComponent(a)+"&ie=utf-8&tn=SE_PCzhidaosszl_6wvraksp","_blank")}else s(this).hasClass("selected")?(s(this).removeClass("selected"),n.send({page:"question",area:"search-relate-word-cancel",action:"click",type:2014,isQ:!!window.isQualityView})):(s(this).addClass("selected"),n.send({page:"question",area:"search-relate-word-add",action:"click",type:2014,isQ:!!window.isQualityView}))})}var s=e("common:widget/js/util/tangram/tangram"),n=e("common:widget/js/util/log/log");t.exports.init=i});