define("question:widget/js/business/business",function(n,i,e){function o(n){"undefined"==typeof window.CLIENT_WIN||window.CLIENT_WIN&&window.CLIENT_WIN.closed?(window.CLIENT_WIN=window.open(n,"clientwin","width=1150,height=680,status=no,resizable=yes"),(null==window.CLIENT_WIN||window.CLIENT_WIN.document.location.href!=n)&&(window.CLIENT_WIN=window.open(n,"clientwin","width=1150,height=680,status=no,resizable=yes"))):window.CLIENT_WIN.focus()}function s(n,i){t.get("/business/api/anyonline",{businessId:n},function(e){var s=!!e.data[n];s?(i.html("向TA咨询"),i.removeClass("disabled"),u.fire("login.check",{isLogin:function(){i.click(function(i){return i.preventDefault(),F.context("user").isBusiness?void l.alert("为保证公正性，您不能对机构进行咨询（包括您的机构）"):(c.send({module:"question",page:"question",area:"hangjia-chat-btn",action:"goChat",isOnline:1}),void o("/business/client?businessId="+n))})},noLogin:function(){i.click(function(i){i.preventDefault(),u.fire("login.log",{onLoginSuccess:function(){o("/business/client?businessId="+n)}})})}}),a||(a=!0)):(i.html("给TA留言"),u.fire("login.check",{isLogin:function(){i.click(function(i){return i.preventDefault(),F.context("user").isBusiness?void l.alert("为保证公正性，您不能对机构进行咨询（包括您的机构）"):(c.send({module:"question",page:"question",area:"hangjia-chat-btn",action:"goChat",isOnline:0}),void o("/business/client?businessId="+n))})},noLogin:function(){i.click(function(i){i.preventDefault(),u.fire("login.log",{onLoginSuccess:function(){o("/business/client?businessId="+n)}})})}}),a||(a=!0))},"json")}var t=n("common:widget/js/util/tangram/tangram"),u=n("common:widget/js/util/event/event"),c=n("common:widget/js/util/log/log"),l=n("common:widget/js/ui/dialog/dialog"),a=!1;window.CLIENT_WIN,e.exports={checkOnline:s}});